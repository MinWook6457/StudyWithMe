{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "https://studywithme.store/api/$1" 
      },
      {
        "src": "/kakao/callback",
        "dest": "/kakao/callback"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html" 
      }
    ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "https://withroom.store"
          },
          {
            "key": "Access-Control-Allow-Headers",
            "value": "Content-Type"
          },
          {
            "key": "Access-Control-Allow-Methods",
            "value": "GET,OPTIONS"
          }
        ]
      }
    ]
  }
  